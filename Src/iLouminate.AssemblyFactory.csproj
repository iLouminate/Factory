<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Company>iLouminate</Company>
    <Authors>iLouminate</Authors>
    <Description>Supports Interface based Factories, also supports Dependency Injection.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageProjectUrl>https://github.com/iLouminate/Factory</PackageProjectUrl>
    <RepositoryUrl>https://github.com/iLouminate/Factory</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>Factory, Dependency Injection, Interface Factory, Factory Pattern</PackageTags>
    <PackageReleaseNotes />
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
